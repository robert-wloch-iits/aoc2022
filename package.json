{
  "name": "vite-vue3-jest",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "serve": "run-s build preview",
    "test": "run-p -l test:unit:coverage test:e2e:headless",
    "test:parallel-aggregate": "run-p --aggregate-output -l test:unit test:e2e:headless",
    "test:e2e": "testcafe chrome tests/e2e/ --hostname localhost --app 'pnpm dev'",
    "test:e2e:headless": "E2E_RUN=true testcafe chrome:headless tests/e2e/ --hostname localhost --app 'pnpm dev'",
    "test:unit": "jest --clearCache && jest",
    "test:unit:coverage": "jest --clearCache && jest --coverage"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "vue": "^3.2.35"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "^2.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^26.6.3",
    "prettier": "^2.6.2",
    "testcafe": "^1.18.6",
    "ts-jest": "^26.5.6",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-eslint": "^1.6.1",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.3.0"
  }
}
